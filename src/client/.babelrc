{
  "presets": [
    [
      "react"
    ],
    [
      "es2015",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", {"removeImport": true}]
      ]
    }
  }
}